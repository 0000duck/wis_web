            position: absolute;
            top: 20%;
            right: -17px;
        }

        /*箭頭左*/
        .info_border .arrow_l_int {
            width: 0px;
            height: 0px;
            border-width: 10px;
            border-style: solid;
            border-color: transparent #333 transparent transparent;
            position: absolute;
            top: 20%;
            left: -20px;
        }
        /*箭頭左-邊框*/
        .info_border .arrow_l_out {
            width: 0px;
            height: 0px;
            border-width: 10px;
            border-style: solid;
            border-color: transparent #fff transparent transparent;
            position: absolute;
            top: 20%;
            left: -19px;
        }

        /*箭頭上*/
        .info_border .arrow_t_int {
            width: 0px;
            height: 0px;
            border-width: 10px;
            border-style: solid;
            border-color: transparent transparent #333 transparent;
            position: absolute;
            top: -20px;
            left: 25px;
        }
        /*箭頭上-邊框*/
        .info_border .arrow_t_out {
            width: 0px;
            height: 0px;
            border-width: 10px;
            border-style: solid;
            border-color: transparent transparent #fff transparent;
            position: absolute;
            top: -19px;
            left: 25px;
        }

        /*箭頭下*/
        .info_border .arrow_b_int {
            width: 0px;
            height: 0px;
            border-width: 10px;
            border-style: solid;
            border-color: #333 transparent transparent transparent;
            position: absolute;
            bottom: -20px;
            right: 30px;
        }
        /*箭頭下-邊框*/
        .info_border .arrow_b_out {
            width: 0px;
            height: 0px;
            border-width: 10px;
            border-style: solid;
            border-color: #fff transparent transparent transparent;
            position: absolute;
            bottom: -17px;
            right: 30px;
        }
</style>

<script>
    var Layout = angular.module('Layout', ['ngMaterial', 'ngMessages', 'material.svgAssetsCache']);
    Layout.controller('Layout', function ($scope, $http, $interval, $mdDialog) {

        $scope.showAlert = function (a, b) {
            $scope.isQuery = false;
            $mdDialog.show(
                $mdDialog.alert()
                    .parent(angular.element(document.querySelector('#popupContainer')))
                    .clickOutsideToClose(true)
                    .title(a)
                    .textContent(b)
                    .ariaLabel('Information')
                    .ok('GOT IT!')
                //.targetEvent(ev)
            );
        }

        $scope.init = function () {
            $('.dragToolBox').draggable();
            $('.dragSelectMode').draggable();
            $('.dragInfo').draggable();

            $(".dragToolBox").css('left', $(window).width() * 2 / 3).css('top', 70);
            $(".dragSelectMode").css('left', $(window).width() * 2 / 3).css('top', 70);
            $.contextMenu({
                // define which elements trigger this menu
                selector: ".layoutContent",
                //trigger: 'none',
                // define the elements of the menu
                items: {
                    add: {
                        name: "Add", callback: function (key, opt) {
                            $scope.disableID = false;
                            $scope.targetHeight = '50';
                            $scope.targetWidth = '50';
                            $scope.targetTop = $scope.mouseTop;
                            $scope.targetLeft = $scope.mouseLeft;
                            $scope.targetZIndex = '1';
                            $scope.targetID = '';
                            $scope.targetCaption = '';

                            $scope.isDragEditWindowShow = true;
                            $scope.addMode = true;
                            $scope.$apply();
                            // $('.dragToolBox ').height(400);
                        }
                    },
                    edit: {
                        name: "Edit", callback: function (key, opt) {
                            $scope.disableID = true;
                            $scope.addMode = false;
                            $scope.isDragEditWindowShow = true;
                            $scope.$apply();
                            //$('.dragToolBox ').height(320);
                        }
                    },
                    remove: {
                        name: "Remove", callback: function (key, opt) {
                            $('#' + $scope.targetID).remove();
                        }
                    }
                }

            });



            $('.layoutContent').mousedown(function (e) {
                if (e.button == 2) {

                    //$('.layoutContent').contextMenu();
                    $scope.mouseTop = e.pageY + document.documentElement.scrollTop - $('.layoutContent').offset().top;
                    $scope.mouseLeft = e.pageX + document.documentElement.scrollLeft - $('.layoutContent').offset().left;
                    //$scope.targetCaption = e.pageY + '  ' + document.documentElement.scrollTop + '   ' + $('.layoutContent').offset().top + '  ' + parseInt($('.get-top-offset').css('height').replace('px', ''));
                    if (e.target.id == '') {
                        e.target = $(e.target).parent()[0];
                    }
                    $scope.targetID = e.target.id;
                    $scope.targetHeight = e.target.height;
                    $scope.targetWidth = e.target.width;
                    $scope.targetTop = e.target.top;
                    $scope.targetLeft = e.target.left;
                    $scope.targetCaption = $(e.target).text();
                    $scope.targetZIndex = e.target.style.zIndex;
                }
            });
        }

        $scope.TypeModel = [{ type: 'SQUARE' },
                            { type: 'LABEL' },
                            { type: 'BLANK' },
                            { type: 'POINT' },
                            { type: 'INFO_R' },
                            { type: 'INFO_L' },
                            { type: 'INFO_T' },
                            { type: 'INFO_B' }];

        $scope.addToLayout = function () {
            $scope.isDragEditWindowShow = false;

            printElementForEdit($scope.targetID, $scope.targetHeight, $scope.targetWidth, $scope.targetTop, $scope.targetLeft, $scope.targetCaption, $scope.targetZIndex, $scope.targetType);
            $('#' + $scope.targetID).resizable({ grid: [5, 5] }).draggable();
        }

        $scope.getLayout = function (name) {
            $http.post("@Url.Action("GetLayout", "Home")", { Name: $scope.LayoutName }).success(function (response) {
                if (response.Status == '200') {
                    $.each(response.Message, function (index, d) {
                    // if already have elements, remove all
                    $('#' + d.id).remove();
                    printElementForEdit(d.id, d.height, d.width, d.top, d.left, d.caption, d.zIndex, d.type);

                    });
                } else {
                    $scope.showAlert('Error', response.Message);
                }
            }).error(function (response) {
                //alert(response);
            });


        }

        $scope.saveLayout = function () {

            var listObject = [];
            var divType = '';
            $('.ui-resizable').each(function () {

                if ($(this).hasClass('squareType')) {
                    divType = 'SQUARE';
                } else if ($(this).hasClass('labelType')) {
                    divType = 'LABEL';
                } else if ($(this).hasClass('blankType')) {
                    divType = 'BLANK';
                } else if ($(this).hasClass('pointType')) {
                    divType = 'POINT';
                } else if ($(this).hasClass('info_border')) {
                    if ($(this).children().hasClass('arrow_r_int')) {
                        divType = 'INFO_R';
                    } else if ($(this).children().hasClass('arrow_l_int')) {
                        divType = 'INFO_L';
                    } else if ($(this).children().hasClass('arrow_t_int')) {
                        divType = 'INFO_T';
                    } else if ($(this).children().hasClass('arrow_b_int')) {
                        divType = 'INFO_B';
                    }
                }

                listObject.push({
                    id: this.id,
                    height: this.style.height,
                    width: this.style.width,
                    top: this.style.top,
                    left: this.style.left,
                    caption: $(this).children("p").text(),
                    type: divType,
                    zIndex: this.style.zIndex,
                });
            });
            $http.post("@Url.Action("SaveLayout", "Home")", { Name: $scope.LayoutName, listObject: JSON.stringify(listObject) }).success(function (response) {
                if (response.Status == '200') {
                    //alert(response.detail);
                    $scope.showAlert('Info', response.Message);
                } else {
                    //resetTimeout();
                    $scope.showAlert('Error', response.Message);
                    $scope.ShowSelectMode = false;
                    $scope.edit = false;
                }
            }).error(function (response) {
                $scope.showAlert('Error', response);
            });

        };

        $scope.$watch('edit', function (newValue, oldValue) {
            if (newValue == false) {
                $('.layoutContent').contextMenu(false);
                $('.layoutContent div').remove();
                $scope.isDragEditWindowShow = false;

                $scope.getLayout($scope.LayoutName);
            } else {
                $('.layoutContent').contextMenu(true);
                $('.layoutContent div').resizable({ grid: [5, 5] }).draggable();
            }
        });
        $scope.init();

        function printElementForEdit(Id, height, width, top, left, caption, zIndex, type) {
            var div = document.createElement('div');
            var DivID;
            div.style.height = height;
            div.style.width = width;
            div.style.top = top;
            div.style.left = left;
            div.style.zIndex = zIndex;
            div.id = Id;

            var p = document.createElement('p');


            switch (type) {
                case 'SQUARE':
                    $(div).addClass('squareType');
                    $(p).addClass('objectCaption');
                    break;
                case 'LABEL':
                    $(div).addClass('labelType');
                    $(p).addClass('objectCaption');
                    break;
                case 'BLANK':
                    $(div).addClass('blankType');
                    $(p).addClass('objectCaption');
                    break;
                case 'POINT':
                    $(div).addClass('pointType');
                    $(p).addClass('objectCaption');
                    $(div).hover(
                      function () {
                          var c = '';
                          var t = '';

                          DivID = div.id; /* POINT  caption byEASY*/
                          switch (DivID) {
                              case 'pointT2SB':
                                  PointCaption = 'T2SB' + ' 貨車:' + $scope.T2_SB.Truck + ' 貨櫃:' + $scope.T2_SB.Container + ' 訪客:' + $scope.T2_SB.Guest;
                                  break;
                              case 'pointT2Delivery':
                                  PointCaption = 'T2出貨' + ' 貨車:' + $scope.T2_Delivery.Truck + '貨櫃:' + $scope.T2_Delivery.Container + ' 訪客:' + $scope.T2_Delivery.Guest;
                                  break;
                              case 'pointT2Receive':
                                  PointCaption = 'T2收貨' + ' 貨車:' + $scope.T2_Receive.Truck + '貨櫃:' + $scope.T2_Receive.Container + ' 訪客:' + $scope.T2_Receive.Truck;
                                  break;
                              case 'pointT2WH':
                                  PointCaption = 'T2WH' + '貨車:' + $scope.T2_WH.Truck + '貨櫃:' + $scope.T2_WH.Container + '訪客:' + $scope.T2_WH.Guest;
                                  break;
                              case 'pointT2CP':
                                  PointCaption = '中櫃場' + ' 貨車:' + $scope.T2_Center.Truck + '貨櫃:' + $scope.T2_Center.Container + ' 訪客:' + $scope.T2_Center.Guest;
                                  break;
                              case 'pointT1SB':
                                  PointCaption = 'T1SB' + '貨車:' + $scope.T1_SB.Truck + '貨櫃:' + $scope.T1_SB.Container + '訪客:' + $scope.T1_SB.Guest;
                                  break;
                              case 'pointT1WH':
                                  PointCaption = 'T1WH' + ' 貨車:' + $scope.T1_WH.Truck + '貨櫃:' + $scope.T1_WH.Container + ' 訪客:' + $scope.T1_WH.Guest;
                                  break;
                              case 'pointT1Receive':
                                  PointCaption = 'T1收貨' + ' 貨車:' + $scope.T1_Receive.Truck + '貨櫃:' + $scope.T1_Receive.Container + ' 訪客:' + $scope.T1_Receive.Guest;
                                  break;
                              case 'pointT1Delivery':
                                  PointCaption = 'T1出貨' + ' 貨車:' + $scope.T1_Delivery.Truck + '貨櫃:' + $scope.T1_Delivery.Container + ' 訪客:' + $scope.T1_Delivery.Guest;
                                  break;
                              case 'pointT1Parking':
                                  PointCaption = 'Parking' + ' 貨車:' + $scope.T1_Parking.Truck + '貨櫃:' + $scope.T1_Parking.Container + ' 訪客:' + $scope.T1_Parking.Guest;
                                  break;
                              default:
                                  PointCaption = '';
                                  break;
                          }
                          /*----------------*/
                          printElementForEdit('hoverInfo', '110', '60', (parseInt(this.style.top.replace('px', '')) - 160).toString() + 'px', (parseInt(this.style.left.replace('px', '')) - 45).toString() + 'px', PointCaption, '9', 'INFO_B');
                      }, function () {
                          $('#hoverInfo').remove();
                      }
                    ).click(function () {
                        $http.post("@Url.Action("GetCarDetail", "Home")", { DivPoint: DivID }).success(function (response) {
                            //byEASY for EDC data
                            var detail = JSON.parse(response);

                            $scope.carLists = detail;
                            $scope.isInfoShow = true;
                           // $scope.$apply();
                        }).error(function (response) {
                            //alert(response);
                        });
                    });

                break;
            default:

                var span = document.createElement('span');
                var span1 = document.createElement('span');
                $(p).addClass('infoCaption');
                switch (type) {
                    case 'INFO_R':
                        $(span).addClass('arrow_r_int');
                        $(span1).addClass('arrow_r_out');
                        break;
                    case 'INFO_L':
                        $(span).addClass('arrow_l_int');
                        $(span1).addClass('arrow_l_out');
                        break;
                    case 'INFO_T':
                        $(span).addClass('arrow_t_int');
                        $(span1).addClass('arrow_t_out');
                        break;
                    case 'INFO_B':
                        $(span).addClass('arrow_b_int');
                        $(span1).addClass('arrow_b_out');
                        //$(div).children("p").remove();
                        //$(div).text(caption);
                        break;
                }
                $(span).appendTo(div);
                $(span1).appendTo(div);
                $(div).addClass('info_border');
                break;
        }

        $(p).text(caption);
        $(p).appendTo(div);
        $(div).appendTo('.layoutContent');



    };
        $scope.getAreaCount = function () {
            $http.post("@Url.Action("GetAreaCount", "Home")", {}).success(function (response) {
                if (response.Status == '200') {
                    //T1
                    $scope.T1_WH = response.Message.T1_WH;
                    $scope.T1_Delivery = response.Message.T1_Delivery;
                    $scope.T1_Receive = response.Message.T1_Receive;
                    $scope.T1_SB = response.Message.T1_SB;
                    $scope.T1_Parking = response.Message.T1_Parking;

                    //T2
                    $scope.T2_WH = response.Message.T2_WH;
                    $scope.T2_Delivery = response.Message.T2_Delivery;
                    $scope.T2_Receive = response.Message.T2_Receive;
                    $scope.T2_SB = response.Message.T2_SB;
                    $scope.T2_Center = response.Message.T2_Center;
                    //Total
                    $scope.Total_Truck = response.Message.Total.Guest;
                    $scope.Total_BigTruck = response.Message.Total.Truck//byEASY
                    $scope.Total_Container = response.Message.Total.Container;
                    //$scope.$apply();
                } else {
                    $scope.showAlert('Error', response.Message);
                }
            }).error(function (response) {
                $scope.showAlert('Error', response);
                //alert(response);
            });
        }
        $scope.getAreaCount();
        $interval(function () { $scope.getAreaCount(); }, 3000);

    })
    Layout.controller('Statistice', function ($scope) {

    })

</script>
<div ng-app="Layout" ng-controller="Layout">
    <div class="row">

        <div class='panel panel-default dragInfo' ng-show="isInfoShow" ng-init="isInfoShow = false" style="left:50px;position: absolute; z-index: 99; /*height: 600px; width: 800px; */  -moz-box-shadow: 0px 0px 8px #363636; -webkit-box-shadow: 0px 0px 8px #363636; box-shadow: 0px 0px 8px #363636;">
            <div class='panel-heading'>
                <i class='icon-edit icon-large'></i>
                <strong>詳細資訊</strong>
            </div>
            <div class='panel-body'>

                <table class='table table-striped table-bordered'>
                    <thead>
                        <tr>
                            <th>SN</th>
                            <th>進場日期</th>
                            <th>車號</th>
                            <th>櫃號</th>
                            <th>廠商</th>
                            <th>車輛類型</th>

                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="car in carLists">
                            <td>{{$index+1}}</td>
                            <td>{{car.STARTTIME}}</td>
                            <td>{{car.CAR_ID}}</td>
                            <td>{{car.CONTAINER_ID}}</td>
                            <td>{{car.VENDOR_NAME}}</td>
                            <td>{{car.CAR_TYPE.replace("Container", "貨櫃").replace("Guest", "訪客").replace("Truck", "貨車")}}</td>

                        </tr>

                    </tbody>
                </table>

                </fieldset>
                <div class='form-actions'>
                    <button class='btn btn-default' type='submit' ng-click="isInfoShow = false">關閉</button>

                </div>

            </div>

        </div>

        <div class='panel panel-default dragSelectMode' ng-show="ShowSelectMode">
            <div class='panel-heading'>
                <i class='icon-edit icon-large'></i>
                <strong>名稱</strong>
            </div>
            <div class='panel-body'>
                <form>
                    <fieldset>
                        <legend>Layout Name</legend>
                        <div class='form-group has-success'>
                            <label class='control-label'>名稱</label>
                            <input class='form-control' placeholder='Please input name' ng-model="LayoutName" ng-init="LayoutName = '1'">
                            <p class='help-block'>Layout Name.</p>
                        </div>
                    </fieldset>
                    <div class='form-actions'>
                        <button class='btn btn-default' type='submit' ng-click="saveLayout()">確定</button>

                        <a class='btn' href='#' ng-click="ShowSelectMode = false">取消</a>
                    </div>
                </form>
            </div>
        </div>

        <div class='panel panel-default dragToolBox' ng-show="isDragEditWindowShow">
            <div class='panel-heading'>
                <i class='icon-edit icon-large'></i>
                <strong>工具箱</strong>
            </div>
            <div class='panel-body'>
                <form>

                    <div class='form-group has-success'>
                        <label class='control-label'>ID</label>
                        <input class='form-control' placeholder='Text only' ng-model="targetID" ng-disabled="disableID">
                    </div>

                    <div class='form-group has-success'>
                        <label class='control-label'>顯示名稱</label>
                        <input class='form-control' placeholder='Caption' ng-model="targetCaption">
                        @*<p class='help-block'>Object's Caption.</p>*@
                    </div>
                    <div class='form-group has-success' ng-show="addMode">
                        <label class='control-label'>類型</label>
                        <select class='form-control' ng-model="objectType" ng-options="eachType.type for eachType in TypeModel" ng-change="targetType = objectType.type">
                            <option value="">請選擇...</option>
                        </select>
                    </div>
                    <div class='form-group has-success'>
                        <label class='control-label'>Z-Index</label>
                        <input class='form-control' placeholder='Number only' ng-model="targetZIndex">
                        @*<p class='help-block'>Object's Z-Index.</p>*@
                    </div>


                    <div class='form-actions'>
                        <button class='btn btn-default' type='submit' ng-click="addToLayout()">確定</button>

                        <a class='btn' href='#' ng-click="isDragEditWindowShow = false">取消</a>
                    </div>
                </form>
            </div>
        </div>

        <div class="row">
            <div class="col-md-10 col-md-offset-1 get-offset">
                <div class="panel panel-default">
                    <div class="panel-heading">

                        <h3 class="panel-title pull-left"><strong>廠內即時分布狀態</strong></h3>
                        <button class="btn-xs btn-default pull-right" ng-show="!edit" ng-click="edit=true" ng-init="edit=false">修改</button>
                        <button class="btn-xs btn-default pull-right" ng-show="edit" ng-click="ShowSelectMode = true;">儲存</button>
                        <button class="btn-xs btn-default pull-right" ng-show="edit" ng-click="edit=false">取消</button>
                        <div class="clearfix"></div>
                    </div>
                    <div class="panel-body layoutContent">

                        <form>
                            <div class="">
                            </div>
                        </form>
                    </div>

                </div>

            </div>

        </div>
    </div>
    <div class="row">
        <div class="col-md-2 col-md-offset-1">
            <div class="panel panel-success">
                <div class="panel-heading">

                    <h3 class="panel-title"><strong>合計</strong></h3>
                </div>
                <div class="panel-body detail-form">

                    <form>
                        <div class="col-md-10 col-md-offset-1">
                            <div class="row">
                                <h5>
                                    <span class="label label-danger">T1 & T2</span>
                                </h5>
                            </div>
                            <div class="row">

                                <ul class="list-group">
                                    <li class="list-group-item">
                                        <span class="badge">{{Total_Container}}</span>
                                        廠內貨櫃
                                    </li>
                                    <li class="list-group-item">
                                        <span class="badge">{{Total_BigTruck}}</span>
                                        廠內貨車
                                    </li>
