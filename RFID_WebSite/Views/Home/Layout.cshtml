<style>
    .layoutContent {
        /*height: 400px;*/
        background-image: url("../Images/layout1085.jpg");
        height: 339px;
        width: 1085px;
    }

    .detail-form {
        /*background:rgb(13, 8, 60);*/
    }

    .dragToolBox {
        z-index: 99;
        /*height: 700px;*/
        width: 200px;
        position: absolute;
    }

    .dragSelectMode {
        z-index: 99;
        width: 300px;
        position: absolute;
